HOME                  DEFAULT=@{HOME}

# XDG base directory specification
XDG_CACHE_HOME        DEFAULT="${HOME}/.cache"
XDG_CONFIG_HOME       DEFAULT="${HOME}/.config"
XDG_DATA_HOME         DEFAULT="${HOME}/.local/share"
XDG_STATE_HOME        DEFAULT="${HOME}/.local/var/lib"
XDG_LIB_HOME          DEFAULT="${HOME}/.local/lib"
XDG_LOG_HOME          DEFAULT="${HOME}/.local/var/log"
LD_LIBRARY_PATH       DEFAULT="${XDG_LIB_HOME}"
XDG_DATA_DIRS         DEFAULT="/usr/local/share:/usr/share" OVERRIDE="${XDG_DATA_DIRS}"
XDG_DATA_DIRS         DEFAULT="${XDG_DATA_HOME}:${XDG_DATA_DIRS}"

PATH                  DEFAULT="${PATH}:${HOME}/.local/bin"

# XAUTHORITY            DEFAULT="${XDG_RUNTIME_DIR}/Xauthority"
XINITRC               DEFAULT="${XDG_CONFIG_HOME}/X11/xinitrc"
XSERVERRC             DEFAULT="${XDG_CONFIG_HOME}/X11/xserverrc"

ANDROID_SDK_HOME      DEFAULT="${XDG_CONFIG_HOME}/android"
ATOM_HOME             DEFAULT="${XDG_DATA_HOME}/atom"
BASH_COMPLETION_USER_FILE DEFAULT="${XDG_CONFIG_HOME}/bash-completion/bash_completion"
BITWARDEN_APPDATA_DIR  DEFAULT="${XDG_DATA_HOME}/bitwarden/desktop"
BITWARDENCLI_APPDATA_DIR  DEFAULT="${XDG_DATA_HOME}/bitwarden/cli"
BUNDLE_USER_CONFIG    DEFAULT="${XDG_CONFIG_HOME}/bundle"
BUNDLE_USER_CACHE     DEFAULT="${XDG_CACHE_HOME}/bundle"
BUNDLE_USER_PLUGIN    DEFAULT="${XDG_DATA_HOME}/bundle"
CARGO_HOME            DEFAULT="${XDG_DATA_HOME}/cargo"
DOCKER_CONFIG         DEFAULT="${XDG_CONFIG_HOME}/docker"
ELINKS_CONFDIR        DEFAULT="${XDG_CONFIG_HOME}/elinks"
GIMP2_DIRECTORY       DEFAULT="${XDG_STATE_HOME}/gimp"
GEM_HOME              DEFAULT="${XDG_DATA_HOME}/gem"
GEM_SPEC_CACHE        DEFAULT="${XDG_CACHE_HOME}/cache"
GOPATH                DEFAULT="${XDG_DATA_HOME}/go"
GNUPGHOME             DEFAULT="${XDG_DATA_HOME}/gnupg"
GRADLE_USER_HOME      DEFAULT="${XDG_DATA_HOME}/gradle"
GTK_RC_FILES          DEFAULT="${XDG_CONFIG_HOME}/gtk-1.0/gtkrc"
GTK2_RC_FILES         DEFAULT="${XDG_CONFIG_HOME}/gtk-2.0/gtkrc"
HISTFILE              DEFAULT="${XDG_DATA_HOME}/bash/history"
INPUTRC               DEFAULT="${XDG_CONFIG_HOME}/readline/inputrc"
_JAVA_OPTIONS         DEFAULT="-Djava.util.prefs.userRoot=\"$XDG_CONFIG_HOME\"/java"
LESSHISTFILE          DEFAULT="${XDG_CACHE_HOME}/less/history"
MPLAYER_HOME          DEFAULT="${XDG_CONFIG_HOME}/mplayer"
MYSQL_HISTFILE        DEFAULT="${XDG_DATA_HOME}/mysql_history"
NPM_CONFIG_USERCONFIG DEFAULT="${XDG_CONFIG_HOME}/npm/npmrc"
NOTMUCH_CONFIG        DEFAULT="${XDG_CONFIG_HOME}/notmuch/notmuchrc"
NMBGIT                DEFAULT="${XDG_DATA_HOME}/notmuch/nmbug"
NVM_DIR               DEFAULT="${XDG_DATA_HOME}/nvm"
NUGET_PACKAGES        DEFAULT="${XDG_CACHE_HOME}/nuget/packages"
MAILCAP               DEFAULT="${XDG_CONFIG_HOME}/rtv/mailcap"
PASSWORD_STORE_DIR    DEFAULT="${XDG_DATA_HOME}/pass"
PGPPATH               DEFAULT="${GNUPGHOME}"
PSQLRC                DEFAULT="${XDG_CONFIG_HOME}/pg/psqlrc"
PSQL_HISTORY          DEFAULT="${XDG_CACHE_HOME}/pg/psql_history"
PGPASSFILE            DEFAULT="${XDG_CONFIG_HOME}/pg/pgpass"
PGSERVICEFILE         DEFAULT="${XDG_CONFIG_HOME}/pg/pg_service.conf"
PYLINTHOME            DEFAULT="${XDG_CACHE_HOME}/pylint"
PYTHON_EGG_CACHE      DEFAULT="${XDG_CACHE_HOME}/python-eggs"
RUSTUP_HOME           DEFAULT="${XDG_DATA_HOME}/rustup"
RXVT_SOCKET           DEFAULT="${XDG_RUNTIME_DIR}/urxvtd"
SCREENRC              DEFAULT="${XDG_CONFIG_HOME}/screen/screenrc"
STACK_ROOT            DEFAULT="${XDG_DATA_HOME}/stack"
TASKDATA              DEFAULT="${XDG_DATA_HOME}/task"
TASKRC                DEFAULT="${XDG_CONFIG_HOME}/task/taskrc"
TERMINFO              DEFAULT="${XDG_DATA_HOME}/terminfo"
TERMINFO_DIRS         DEFAULT="${XDG_DATA_HOME}/terminfo:/usr/share/terminfo"
TMUX_TMPDIR           DEFAULT="${XDG_RUNTIME_DIR}"
WEECHAT_HOME          DEFAULT="${XDG_CONFIG_HOME}/weechat"
WGETRC                DEFAULT="${XDG_CONFIG_HOME}/wget/wgetrc"
WINEPREFIX            DEFAULT="${XDG_DATA_HOME}/wine/pfx/default"
_Z_DATA               DEFAULT="${XDG_DATA_HOME}/z"
